{
  // Telemetry
  "telemetry.telemetryLevel": "off",
  "intelephense.telemetry.enabled": false,
  // Affinity
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  // Window
  "window.commandCenter": true,
  "window.autoDetectColorScheme": true,
  // Workbench
  "workbench.preferredLightColorTheme": "Catppuccin Latte",
  "workbench.preferredDarkColorTheme": "Catppuccin Mocha",
  "workbench.iconTheme": "vscode-jetbrains-icon-theme-2023-dark",
  // Editor
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": true,
    "scminput": false
  },
  "editor.fontFamily": "JetBrains Mono NL",
  "editor.fontSize": 16,
  "editor.fontWeight": "normal",
  "editor.fontLigatures": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "files.autoSave": "onFocusChange",
  "editor.linkedEditing": true,
  "editor.lineNumbers": "on",
  "editor.gotoLocation.multipleDefinitions": "goto",
  // Explorer
  "explorer.confirmDelete": false,
  "explorer.decorations.colors": true,
  "explorer.decorations.badges": false,
  "explorer.excludeGitIgnore": false,
  "explorer.compactFolders": false,
  // Prettier
  "prettier.requireConfig": true,
  "prettier.useEditorConfig": false,
  // Vim

  // Shell
  // "terminal.integrated.profiles.osx": {
  //     "tmux-shell": {
  //         "path": "tmux",
  //         "args": ["new-session", "-A", "-s", "cursor:${worksxpaceFolder}"]
  //     }
  // },
  // "terminal.integrated.defaultProfile.osx": "tmux-shell",
  "terminal.integrated.scrollback": 10000,
  // Nvim
  // FIF
  "find-it-faster.general.useTerminalInEditor": true,
  "find-it-faster.general.batTheme": "Catppuccin Mocha",
  "go.toolsManagement.autoUpdate": true,
  // Languages
  "html.autoClosingTags": true,
  "javascript.autoClosingTags": true,
  "typescript.autoClosingTags": true,
  "javascript.preferences.renameMatchingJsxTags": true,
  "typescript.preferences.renameMatchingJsxTags": true,
  "php.validate.enable": false,
  "php.suggest.basic": false,
  "cursor.cpp.disabledLanguages": [
    "plaintext",
    "scminput",
    "dotenv",
    "terraform-vars"
  ],
  "files.associations": {
    ".env": "dotenv",
    "*.env.*": "env",
    ".env*": "dotenv",
    "secrets.*": "dotenv",
    "justfile": "just",
    "wrangler.json": "jsonc",
    "*.css": "tailwindcss"
  },
  "editor.tokenColorCustomizations": {
    "[*Light*]": {
      "textMateRules": [
        {
          "scope": "ref.matchtext",
          "settings": {
            "foreground": "#000"
          }
        }
      ]
    },
    "[*Dark*]": {
      "textMateRules": [
        {
          "scope": "ref.matchtext",
          "settings": {
            "foreground": "#fff"
          }
        }
      ]
    },
    "textMateRules": []
  },
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb",
    "*.sqlite": "${capture}.${extname}-*",
    "*.db": "${capture}.${extname}-*",
    "*.sqlite3": "${capture}.${extname}-*",
    "*.db3": "${capture}.${extname}-*",
    "*.sdb": "${capture}.${extname}-*",
    "*.s3db": "${capture}.${extname}-*"
  },
  "dotenv.enableAutocloaking": false,
  "[terraform]": {
    "editor.defaultFormatter": "hashicorp.terraform"
  },
  "[dockerfile]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[dotenv]": {
    "editor.formatOnSave": false,
    "editor.defaultFormatter": null
  },
  "css.lint.unknownAtRules": "ignore",
  "tailwindCSS.experimental.classRegex": [
    ["cva\\(((?:[^()]|\\([^()]*\\))*)\\)", "[\"'`]([^\"'`]*).*?[\"'`]"],
    ["cx\\(((?:[^()]|\\([^()]*\\))*)\\)", "(?:'|\"|`)([^']*)(?:'|\"|`)"],
    ["cn\\(((?:[^()]|\\([^()]*\\))*)\\)", "(?:'|\"|`)([^']*)(?:'|\"|`)"],
    ["\\b\\w*[cC]lassName=[\"'`]([^\"'`]*).*?[\"'`]"]
  ],
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[ignore]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  }
}
